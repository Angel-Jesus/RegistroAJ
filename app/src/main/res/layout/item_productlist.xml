<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/fecha_v"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:background="@drawable/drawtabla"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/productos_v"
        android:layout_width="170dp"
        android:layout_height="60dp"
        android:background="@drawable/drawtabla"
        android:gravity="center_horizontal"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/fecha_v"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/precio_v"
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:background="@drawable/drawtabla"
        android:gravity="center_horizontal"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/productos_v"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cantidad_v"
        android:layout_width="80dp"
        android:layout_height="60dp"
        android:background="@drawable/drawtabla"
        android:gravity="center_horizontal"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/precio_v"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>